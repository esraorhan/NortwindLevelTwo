@model CategoryListViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Kategori</h1>

<h1>Index</h1>
<table class="table table-responsive-lg">
    <thead>
        <tr>
            <th>ID</th>
            <th>Ürün Adı</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Categories)
        {
            <tr>
                <td>@item.CategoryID</td>
                <td>@item.CategoryName</td>            
            </tr>
        }

    </tbody>
</table>

